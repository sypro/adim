{
	"autoload": {
		"psr-0": {
			"core": "common/modules"
		},
		"classmap": [
			"vendor/yiisoft/yii/framework/",
			"vendor/MetalGuardian/yii-file-processor",
			"vendor/karagodin/MaintenanceMode",
			"vendor/malyshev/yii-debug-toolbar",
			"vendor/metalguardian/yii-date-time-picker",
			"vendor/MetalGuardian/YiiBooster"
		]
	},
	"repositories": [
		{
			"type":"package",
			"package": {
				"name": "MetalGuardian/yii-file-processor",
				"version":"dev",
				"source": {
					"url": "https://github.com/MetalGuardian/yii-file-processor.git",
					"type": "git",
					"reference":"dev"
				}
			}
		},
		{
			"type":"package",
			"package": {
				"name": "karagodin/MaintenanceMode",
				"version":"master",
				"source": {
					"url": "https://github.com/karagodin/MaintenanceMode.git",
					"type": "git",
					"reference":"master"
				}
			}
		},
		{
			"type":"package",
			"package": {
				"name": "MetalGuardian/YiiBooster",
				"version":"dev",
				"source": {
					"url": "https://github.com/MetalGuardian/YiiBooster.git",
					"type": "git",
					"reference":"dev"
				}
			}
		}
	],
	"require":{
		"php": ">=5.3.0",
		"yiisoft/yii": "dev-master",
		"MetalGuardian/YiiBooster": "dev",
		"malyshev/yii-debug-toolbar": "dev-master",
		"MetalGuardian/yii-file-processor": "dev",
		"karagodin/MaintenanceMode": "dev-master",
		"MetalGuardian/yii-date-time-picker": "dev-master"
	}
}

{
	"autoload": {
		"psr-0": {
			"core": "common/modules"
		},
		"classmap": [
			"vendor/karagodin/MaintenanceMode",
			"vendor/malyshev/yii-debug-toolbar",
			"vendor/clevertech/yii-booster/src"
		]
	},
	"repositories": [
		{
			"type":"package",
			"package": {
				"name": "karagodin/MaintenanceMode",
				"version":"master",
				"source": {
					"url": "https://github.com/karagodin/MaintenanceMode.git",
					"type": "git",
					"reference":"master"
				}
			}
		}
	],
	"scripts": {
		"pre-install-cmd": [
			"core\\Callback::preInstall"
		],
		"post-install-cmd": [
			"core\\Callback::postInstall"
		],
		"pre-update-cmd": [
			"core\\Callback::preUpdate"
		],
		"post-update-cmd": [
			"core\\Callback::postUpdate"
		],
		"post-package-install": [
			"core\\Callback::postPackageInstall"
		],
		"post-package-update": [
			"core\\Callback::postPackageUpdate"
		]
	},
	"require":{
		"php": ">=5.3.0",
		"yiisoft/yii": "dev-master",
		"clevertech/yii-booster": "1.1.0.*@dev",
		"malyshev/yii-debug-toolbar": "dev-master",
		"metalguardian/yii-file-processor": "dev-master",
		"karagodin/MaintenanceMode": "dev-master",
		"metalguardian/yii-date-time-picker": "dev-master",
		"phpmailer/phpmailer": "v5.2.7"
 	}
}
